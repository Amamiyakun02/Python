   "source": [
    "def sinusgenerator(amp,fre,tAkhir,theta):\n",
    "    t = np.arange(0,tAkhir,0.1)\n",
    "    y = amp *np.sin(2*fre*t + np.deg2rad(theta))\n",
    "    return t,y\n",
    "\n",
    "t,y = sinusgenerator(1,1,4,0)\n",
    "\n",
    "plt.plot(t,y)\n",
    "\n",
    "plt.axis([0,4,-2,2])\n",
    "plt.show()"
   ]
  }
